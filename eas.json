{
  "cli": {
    "version": ">= 15.0.12",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "android": {
        "gradleCommand": ":app:bundleRelease",
        "withoutCredentials": false,
        "buildType": "apk"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
